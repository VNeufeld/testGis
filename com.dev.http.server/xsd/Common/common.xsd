			<xs:documentation>The code of the airport</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AirportName" type="xs:string">
		<xs:annotation>
			<xs:documentation>The name of the airport</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AirportId" type="xs:int">
