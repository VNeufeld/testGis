	<xs:element name="Success">
		<xs:annotation>
			<xs:documentation>0 = error, 1 = OK</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:short">
				<xs:pattern value="0|1"/>
				<xs:enumeration value="0"/>
				<xs:enumeration value="1"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="VersionBackend" type="xs:string">
		<xs:annotation>
			<xs:documentation>The backends version</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="VersionRequestor" type="xs:string">
		<xs:annotation>
			<xs:documentation>The Requestor version</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="VersionOpenInterface" type="xs:string">
		<xs:annotation>
			<xs:documentation>The OpenInterface definition version</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="SupplierCategory">
		<xs:annotation>
			<xs:documentation>The suppliers car category</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Position" type="xs:int">
		<xs:annotation>
			<xs:documentation>The position from where to start</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AreaId" type="xs:int">
		<xs:annotation>
			<xs:documentation>The id of the area as fetched from GetAreas</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Member">
		<xs:annotation>
			<xs:documentation>Boolean if the Service Catalog is valid for Members</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:int">
				<xs:pattern value="0|1"/>
				<xs:enumeration value="0"/>
				<xs:enumeration value="1"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="PartnerInfo" type="xs:string"/>
	<xs:element name="NonMember">
		<xs:annotation>
			<xs:documentation>Boolean if the Service Catalog is valid for NonMembers</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:int">
				<xs:pattern value="0|1"/>
				<xs:enumeration value="0"/>
				<xs:enumeration value="1"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="SupplierBookNo" type="xs:string">
		<xs:annotation>
			<xs:documentation>The booking number of the supplier</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="BookingConfirmed">
		<xs:annotation>
			<xs:documentation>States if the booking is OK = 1 or RQ = 0</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:int">
				<xs:pattern value="0|1|.{0}"/>
				<xs:enumeration value="0"/>
				<xs:enumeration value="1"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="BookingId" type="xs:string">
		<xs:annotation>
			<xs:documentation>The booking id</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="BookingNo" type="xs:string">
		<xs:annotation>
			<xs:documentation>The booking number</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ErrorNo" type="xs:string">
		<xs:annotation>
			<xs:documentation>The number of the error. See Documentation for a description</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ErrorText" type="xs:string">
		<xs:annotation>
			<xs:documentation>The description of the error</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ErrorType" type="xs:string">
		<xs:annotation>
			<xs:documentation>The type of the error</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CreationUser" type="xs:string">
		<xs:annotation>
			<xs:documentation>The user who has created the booking</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CreationDate">
		<xs:annotation>
			<xs:documentation>The date the booking has been created</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:date">
				<xs:pattern value="\d{4}-\d{2}-\d{2}|.{0}"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="CreationTime">
		<xs:annotation>
			<xs:documentation>The time the booking has been created</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="\d{2}:\d{2}|.{0}"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="StationForceInfo" type="xs:string">
		<xs:annotation>
			<xs:documentation>If the field is not empty the text must be shown before the booking can be finished. It contains information about the station.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="BookDate">
		<xs:annotation>
			<xs:documentation>The date when the booking is made. This is not the pick up date</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:date">
				<xs:pattern value="\d{4}-\d{2}-\d{2}"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="ServiceCatalogCode" type="xs:string">
		<xs:annotation>
			<xs:documentation>The CRS productcode of the service catalog</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ServiceCatalogId" type="xs:int">
		<xs:annotation>
			<xs:documentation>The service catalog id </xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Module">
		<xs:annotation>
			<xs:documentation>the desired renting Module (1=CAR; 11=TRUCK - default is CAR)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:int">
					<xs:attribute ref="com:ReferenceStation"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="PayType" type="xs:int">
		<xs:annotation>
			<xs:documentation>the desired PayType (-1=BOTH; 1=PREPAID; 2=PAY ON ARRIVAL - default is BOTH)</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Quantity" type="xs:int">
		<xs:annotation>
			<xs:documentation>The count of stations</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="OneWayCarStatus">
		<xs:annotation>
			<xs:documentation>0 = on request, 13 = available.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:int">
				<xs:pattern value="0|1|13"/>
				<xs:enumeration value="0"/>
				<xs:enumeration value="1"/>
				<xs:enumeration value="13"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Status">
		<xs:annotation>
			<xs:documentation>0 = on request, 13 = available, 1= Stop</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:int">
				<xs:pattern value="0|1|13"/>
				<xs:enumeration value="0"/>
				<xs:enumeration value="1"/>
				<xs:enumeration value="13"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="SupplierDeliveryStatus">
		<xs:annotation>
			<xs:documentation>0 = on request, 13 = available, 1= Stop</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:int">
				<xs:pattern value="0|1|13"/>
				<xs:enumeration value="0"/>
				<xs:enumeration value="1"/>
				<xs:enumeration value="13"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="Street" type="xs:string">
		<xs:annotation>
			<xs:documentation>The street</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="BrokerId" type="xs:int">
		<xs:annotation>
			<xs:documentation>A broker id</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Broker">
		<xs:annotation>
			<xs:documentation>Broker booking flag</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:int">
				<xs:pattern value="0|1"/>
				<xs:enumeration value="0"/>
				<xs:enumeration value="1"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="CRCardValidYY">
		<xs:annotation>
			<xs:documentation>The credit card valid year</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="\d{4}"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="CRCardValidMM">
		<xs:annotation>
			<xs:documentation>The credit card valid month</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="\d{2}"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="CRCardType" type="xs:string">
		<xs:annotation>
			<xs:documentation>The credit card type</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CRCardVendor" type="xs:string">
		<xs:annotation>
			<xs:documentation>The credit card vendor</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CRCardNumber" type="xs:string">
