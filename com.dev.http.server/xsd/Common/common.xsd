	<xs:element name="Payment">
		<xs:annotation>
			<xs:documentation>enumeration of an payment</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="abc"/>
				<xs:enumeration value="def"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="PaymentType">
		<xs:annotation>
			<xs:documentation>desccribes an payment which may be provided to the customer</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="com:PaymentId"/>
				<xs:element ref="com:Payment"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="RequestPaymentInformation">
		<xs:annotation>
			<xs:documentation>show the provided ways of payment</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:int">
				<xs:pattern value="0|1"/>
				<xs:enumeration value="0"/>
				<xs:enumeration value="1"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="PayAmount">
		<xs:annotation>
			<xs:documentation>The paypal price</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:pattern value="\d{1,6},\d{2}|.{0}"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="PayerId" type="xs:string">
		<xs:annotation>
			<xs:documentation>The payer ID ( Paypal )</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="TransactionId" type="xs:string">
		<xs:annotation>
			<xs:documentation>The transactionID ( Paypal )</xs:documentation>
		</xs:annotation>
	</xs:element>

</xs:schema>
