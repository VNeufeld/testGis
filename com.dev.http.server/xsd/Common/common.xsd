			<xs:documentation>The credit card number</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CRCardTresorNo" type="xs:string">
		<xs:annotation>
			<xs:documentation>The credit card PCI save number</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CRCardCvc" type="xs:string">
		<xs:annotation>
			<xs:documentation>The credit card cvc code</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="BankName" type="xs:string">
		<xs:annotation>
			<xs:documentation>The bank name</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="BankCity" type="xs:string">
		<xs:annotation>
			<xs:documentation>The bank city</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="BankCode" type="xs:string">
		<xs:annotation>
			<xs:documentation>The bank code</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AccountNumber" type="xs:string">
